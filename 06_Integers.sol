pragma solidity >=0.5.0 <0.9.0;

// int -> Signed integers -> 8-256 bit => int8 to int 256
// uint -> Unsigned integers -> 8-256 bit => uint8 to uint 256 
// By default int is assigned 0
// int range -2^(n-1) to (2^(n-1))-1
// uint range 0 to (2^(n))-1

contract Integers{
    int8 count = -127;
    
}